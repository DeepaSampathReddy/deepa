package com.simplilearn.first;

public class First50NaturalNumbers {
	public static void main(String[] args) {
		int i = 0;
		int j = 0;
		System.out.println("first 50 natural no's");
		for(i=0;i<=50;i++) {
			System.out.print(" "+i);
		}
		System.out.println("first 50 even numbers:");
		for(i=0;i<=50;i++) {
			if(i%2 == 0) {
				System.out.print(" "+i);
			}
		}
		System.out.println("sum of first 10 natural numbers:");
		for(i=0;i<10;i++) {
			
			j=j+i;
			System.out.println(j);
		}
		//System.out.println(j);
	}
}
