package com.simplilearn.first;

public class VowelOrNot {
	public static void main(String[] args) {
		char ch = '';
		switch (ch) {
		case 'a':
		case 'e':
		case 'i':
		case 'o':
		case 'u':
		case 'A':
		case 'E':
		case 'I':
		case 'O':
		case 'U':
			System.out.println("Given letter is Vowel");
			break;
		default:
			throw new IllegalArgumentException("Unexpected value: " + ch);
		}
	}
	
	
}
